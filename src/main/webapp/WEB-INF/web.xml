<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>banko</display-name>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>banko.model.Connexion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>client</servlet-name>
    <servlet-class>banko.model.AllClient</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>client</servlet-name>
    <url-pattern>/client</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>compte</servlet-name>
    <servlet-class>banko.model.AllCompte</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>compte</servlet-name>
    <url-pattern>/compte</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AllTransac</servlet-name>
    <servlet-class>banko.model.AllTransac</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AllTransac</servlet-name>
    <url-pattern>/transac</url-pattern>
  </servlet-mapping>
 
</web-app>